//Write a program to check if a number is palindrome
#include <stdio.h>
int main()
{
    int num,n,rev=0,rem=0;
    printf("Enter the number=");
    scanf("%d",&num);
    n=num;
    while(n!=0)
    {
    rem=n%10;
    rev=rev*10+rem;
    n=n/10;
    }
    if(rev==num)
    {
        printf("The number is palindrome");
    }
    else
    {
        printf("The number is not palindrome");
    }
    return 0;
}
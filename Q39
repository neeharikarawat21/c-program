 //Write a program to find product of odd digits of a number
#include<stdio.h>
int main()
{
    int product=1,i,num,digit;
    printf("Enter the number=");
    scanf("%d",&num);
    while(num>0)
    {
        digit=num%10;
        num=num/10;
        if(digit%2!=0)
        {
            product=product*digit;
        }
    }
    printf("Product of odd digits= %d",product);
    return 0;
}
/*Q57: Find the sum of array elements.
Sample Test Cases:
Input 1:
4
2 4 6 8
Output 1:
20

Input 2:
3
1 1 1
Output 2:
3
*/
#include <stdio.h>
int main()
{
    int arr[50],n,i,count=0;;
    printf("How many elements do you want to enter=");
    scanf("%d",&n);
    printf("enter the elements=");
    for(i=0;i<=n-1;i++)
    {
   scanf("%d",&arr[i]);
    }
    printf("The sum=");
    for(i=0;i<=n-1;i++)
    {
    count=count+arr[i];
    }
    printf("%d",count);
    return 0;
}
 //Write a program to find LCM to two numbers
#include<stdio.h>
int main()
{
 int num1,num2,i,hcf,lcm;
 printf("Enter first number=");
 scanf("%d",&num1);
 printf("Enter the second number=");
 scanf("%d",&num2);
 for(i=1;i<=num1&&i<=num2;i++)
 {
    if(num1%i==0 && num2%i==0)
    {
        hcf=i;
    }
 }
 lcm=(num1*num2)/hcf;
 printf("LCM= %d",lcm);
 return 0;
}
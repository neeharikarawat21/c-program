#include <stdio.h>

int main() {
    char s1[100], s2[100];
    int freq[256] = {0};  // ASCII frequency table

    printf("Enter first string: ");
    fgets(s1, sizeof(s1), stdin);

    printf("Enter second string: ");
    fgets(s2, sizeof(s2), stdin);

    // Count characters from first string
    for (int i = 0; s1[i] != '\0'; i++) {
        if (s1[i] != '\n')     // ignore newline
            freq[(unsigned char)s1[i]]++;
    }

    // Subtract characters of second string
    for (int i = 0; s2[i] != '\0'; i++) {
        if (s2[i] != '\n')
            freq[(unsigned char)s2[i]]--;
    }

    // If all frequencies are zero â†’ anagram
    for (int i = 0; i < 256; i++) {
        if (freq[i] != 0) {
            printf("Strings are NOT anagrams.\n");
            return 0;
        }
    }

    printf("Strings ARE anagrams!\n");
    return 0;
}
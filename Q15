Q15 //Program to check whether the given character is uppercase,lowercase,digit or special character
#include <stdio.h>
int main()
{
char ch;
printf("Enter the character=");
scanf("%c",&ch);
if(ch>='A'&& ch<='Z')
{
    printf("It is an uppercase character");
}
else if(ch>='a'&& ch<='z')
{
    printf ("It is a lower case character");
}
else if(ch>='0'&& ch<='9')
{
    printf ("It is a digit");
}
else 
{
    printf("It is a special character");
}
return 0;
}